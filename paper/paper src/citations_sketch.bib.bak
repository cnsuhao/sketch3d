@ARTICLE{Rivers:2010,
  author = {Rivers, Alec and Durand, Fr\'{e}do and Igarashi, Takeo},
  title = {3D modeling with silhouettes},
  journal = {ACM Trans. Graph.},
  year = {2010},
  volume = {29},
  pages = {1--8},
  number = {4},
  abstract = {We present a new sketch-based modeling approach in which models are
	interactively designed by drawing their 2D silhouettes from different
	views. The core idea of our paper is to limit the input to 2D silhouettes,
	removing the need to explicitly create or position 3D elements. Arbitrarily
	complex models can be constructed by assembling them out of parts
	defined by their silhouettes, which can be combined using CSG operations.
	We introduce a new simplified algorithm to compute CSG solids that
	leverages special properties of silhouette cylinders to convert the
	3D CSG problem into one that can be handled entirely with 2D operations,
	making implementation simpler and more robust. We evaluate our approach
	by modeling a random sampling of man-made objects taken from the
	words in WordNet, and show that all of the tested man-made objects
	can be modeled quickly and easily using our approach.},
  address = {New York, NY, USA},
  doi = {http://doi.acm.org/10.1145/1778765.1778846},
  file = {:sketching\\2010-SIGGRAPH 3D Modeling with Silhouettes.pdf:PDF},
  issn = {0730-0301},
  keywords = {3D modeling, sketch-based modeling, silhouettes, sketching, visual
	hull, variational surfaces},
  owner = {Hongbo},
  publisher = {ACM},
  timestamp = {2016.01.02}
}

