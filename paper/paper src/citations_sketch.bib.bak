@INPROCEEDINGS{Igarashi:1999,
  author = {Takeo Igarashi and Satoshi Matsuoka and Hidehiko Tanaka},
  title = {Teddy: a sketching interface for 3{D} freeform design},
  booktitle = {SIGGRAPH '99},
  year = {1999},
  pages = {409--416},
  abstract = {We present a sketching interface for quickly and easily designing
	freeform models such as stuffed animals and other rotund objects.
	The user draws several 2D freeform strokes interactively on the screen
	and the system automatically constructs plausible 3D polygonal surfaces.
	Our system supports several modeling operations, including the operation
	to construct a 3D polygonal surface from a 2D silhouette drawn by
	the user: it inflates the region surrounded by the silhouette making
	wide areas fat, and narrow areas thin. Teddy, our prototype system,
	is implemented as a Java? program, and the mesh construction is done
	in real-time on a standard PC. Our informal user study showed that
	a first-time user typically masters the operations within 10 minutes,
	and can construct interesting 3D models within minutes.},
  doi = {http://doi.acm.org/10.1145/311535.311602},
  file = {1999-SIGGRAPH Teddy.pdf:sketching\\1999-SIGGRAPH Teddy.pdf:PDF},
  isbn = {0-201-48560-5},
  keywords = {sketching, user interface},
  owner = {Hongbo},
  timestamp = {2016.01.03}
}

